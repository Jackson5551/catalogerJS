<div class="modal fade" id="softdelmodal{{this.id}}" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel"><i class="bi bi-exclamation-triangle"></i> Delete Software?</h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <strong class="text-danger">Are you sure you want to delete this entry:</strong>
                <br/>
                <h2><strong>{{this.name}}</strong></h2>
                <br/>
                <hr />
                <p class="text-muted">Manufacturer</p>
                <strong>{{this.mnfctr}}</strong>
                <br/>
                <p class="text-muted">Version</p>
                <strong>{{this.version}}</strong>
                <br/>
                <p class="text-muted">Format</p>
                <strong>{{this.format}}</strong>
                <br/>
                <p class="text-muted">Number of Media</p>
                <strong>{{this.num_of_media}}</strong>
                <br/>
                <p class="text-muted">Architecture</p>
                <strong>{{this.architecture}}</strong>
                <br/>
                <p class="text-muted">Catagory</p>
                <strong>{{this.catagory}}</strong>
                <br/>
                <p class="text-muted">Product Key</p>
                <strong>{{this.prod_key}}</strong>
                <br />
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cancel</button>
                <a href="/softwaredel/{{this.id}}" type="button" class="btn btn-danger"><i class="bi bi-trash"></i> Delete</a>
            </div>
        </div>
    </div>
</div>