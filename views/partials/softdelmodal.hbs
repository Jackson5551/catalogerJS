<!-- Modal -->
<div class="modal fade" id="softdelmodal{{this.id}}" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Delete Software?</h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                Are you sure you want to delete this entry:
                <br/>
                <strong>{{this.name}}</strong>
                <br/>
                <strong>{{this.mnfctr}}</strong>
                <br/>
                <strong>{{this.version}}</strong>
                <br/>
                <strong>{{this.format}}</strong>
                <br/>
                <strong>{{this.num_of_media}}</strong>
                <br/>
                <strong>{{this.architecture}}</strong>
                <br/>
                <strong>{{this.catagory}}</strong>
                <br/>
                <strong>{{this.prod_key}}</strong>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                <a href="/softwaredel/{{this.id}}" type="button" class="btn btn-danger"><i class="bi bi-trash"></i> Delete</a>
            </div>
        </div>
    </div>
</div>